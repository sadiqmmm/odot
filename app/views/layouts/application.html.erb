<!DOCTYPE html>
<html>
<head>
  <title>Odot</title>
  <%= stylesheet_link_tag    "application", media: "all", "data-turbolinks-track" => true %>
  <%= javascript_include_tag "application", "data-turbolinks-track" => true %>
  <%= csrf_meta_tags %>
</head>
<body>
 
 <% flash.each do |type, messsage| %>
   <div class="alert flash <%= type %>">
     <%= messsage %>
   </div>
 <% end %>

<%= yield %>

</body>
</html>
